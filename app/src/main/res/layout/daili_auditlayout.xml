<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:id="@+id/dailiauditlayout"
    android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:orientation="horizontal"
        android:gravity="center"
        android:layout_marginTop="5dp"
        android:layout_marginRight="10dp"
        >
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/audit"
            android:button="@null"
            android:id="@+id/stateimg"
            android:textSize="13sp"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"/>

        <TextView
            android:id="@+id/imgtitle"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/radioButtons"
            android:layout_alignBottom="@+id/radioButtons"
            android:layout_centerVertical="true"
            android:layout_marginLeft="5dp"
            android:layout_toEndOf="@+id/radioButtons"
            android:background="@null"
            android:gravity="center_vertical"
            android:hint=""
            android:textSize="13sp" />

    </LinearLayout>

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:layout_marginTop="5dp"
        android:background="@color/topbagcolor" />
    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:id="@+id/addresslayout"
        android:visibility="gone"
        android:layout_marginTop="5dp"
        android:layout_marginRight="10dp"
        >

        <RadioButton
            android:id="@+id/radioButtons"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="10dp"
            android:button="@null"
            android:drawablePadding="5dp"
            android:text="推广地址:"
            android:textSize="13sp" />

        <TextView
            android:id="@+id/tuiguangaddr"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/radioButtons"
            android:layout_alignBottom="@+id/radioButtons"
            android:layout_centerVertical="true"
            android:layout_marginLeft="5dp"

            android:layout_toRightOf="@+id/radioButtons"
            android:layout_toLeftOf="@+id/changeinto"
            android:background="@null"
            android:gravity="center_vertical"
            android:textSize="13sp" />
        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="复制"
            android:textSize="13sp"
            android:paddingLeft="7dp"
            android:paddingRight="7dip"
            android:paddingTop="2dip"
            android:paddingBottom="2dip"
            android:layout_gravity="center"
            android:layout_alignParentRight="true"
            android:textColor="@color/white"
            android:background="@drawable/radius_imageview"
            android:id="@+id/changeinto" />
    </RelativeLayout>

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:layout_marginTop="5dp"
        android:visibility="gone"
        android:id="@+id/line"
        android:background="@color/topbagcolor" />
    <RelativeLayout
        android:id="@+id/accountlayoutX"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="5dp"
        android:layout_marginRight="10dp"
        >

        <RadioButton
            android:id="@+id/radioButton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="10dp"
            android:button="@null"
            android:drawablePadding="5dp"
            android:text="代理账号:"
            android:textSize="13sp" />

        <TextView
            android:id="@+id/dailiauditname"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="5dp"
            android:layout_toEndOf="@+id/radioButton"
            android:background="@null"
            android:gravity="center_vertical"
            android:textSize="13sp"
    />

    </RelativeLayout>

    <View
        android:id="@+id/accountline"
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:layout_marginTop="5dp"
        android:background="@color/topbagcolor" />
    <RelativeLayout
        android:id="@+id/accountlayout1"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="5dp"
        android:layout_marginRight="10dp"
        >

        <RadioButton
            android:id="@+id/radioButton1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="10dp"
            android:button="@null"
            android:drawablePadding="5dp"
            android:text="代理类型:"
            android:textSize="13sp" />

        <TextView
            android:id="@+id/dailiaudittype"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/radioButton1"
            android:layout_alignBottom="@+id/radioButton1"
            android:layout_centerVertical="true"
            android:layout_marginLeft="5dp"
            android:layout_toEndOf="@+id/radioButton1"
            android:background="@null"
            android:gravity="center_vertical"
            android:textSize="13sp" />

    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:layout_marginTop="5dp"
        android:background="@color/topbagcolor" />
    <RelativeLayout
        android:id="@+id/accountlayout3"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="5dp"
        android:layout_marginRight="10dp"
        >

        <RadioButton
            android:id="@+id/dailiemail"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="10dp"
            android:button="@null"
            android:drawablePadding="5dp"
            android:text="邮箱地址:"
            android:textSize="13sp" />

        <TextView
            android:id="@+id/auditemail"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/dailiemail"
            android:layout_alignBottom="@+id/dailiemail"
            android:layout_centerVertical="true"
            android:layout_marginLeft="5dp"
            android:layout_toEndOf="@+id/dailiemail"
            android:background="@null"
            android:gravity="center_vertical"
            android:textSize="13sp" />

    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:layout_marginTop="5dp"
        android:background="@color/topbagcolor" />

    <RelativeLayout
        android:id="@+id/accountlayout4"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:visibility="gone"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="5dp"
        android:layout_marginRight="10dp"
        >

        <RadioButton
            android:id="@+id/applyfor"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="10dp"
            android:button="@null"
            android:drawablePadding="5dp"
            android:text="申请理由:"
            android:textSize="13sp" />

        <TextView
            android:id="@+id/applyforvalue"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/applyfor"
            android:layout_alignBottom="@+id/applyfor"
            android:layout_centerVertical="true"
            android:layout_marginLeft="5dp"
            android:layout_toEndOf="@+id/applyfor"
            android:background="@null"
            android:gravity="center_vertical"
            android:textSize="13sp" />

    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:id="@+id/applyline"
        android:visibility="gone"
        android:layout_marginTop="5dp"
        android:background="@color/topbagcolor" />

    <RelativeLayout
        android:id="@+id/accountlayout5"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:visibility="gone"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="5dp"
        android:layout_marginRight="10dp"
        >

        <RadioButton
            android:id="@+id/remark"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="10dp"
            android:button="@null"
            android:drawablePadding="5dp"
            android:text="       备注:"
            android:textSize="13sp" />

        <TextView
            android:id="@+id/remarkvalue"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/remark"
            android:layout_alignBottom="@+id/remark"
            android:layout_centerVertical="true"
            android:layout_marginLeft="5dp"
            android:layout_toEndOf="@+id/remark"
            android:background="@null"
            android:gravity="center_vertical"
            android:textSize="13sp" />

    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:id="@+id/remarkline"
        android:visibility="gone"
        android:layout_marginTop="5dp"
        android:background="@color/topbagcolor" />

    <RelativeLayout
        android:id="@+id/accountlayout6"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:visibility="gone"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="5dp"
        android:layout_marginRight="10dp"
        >

        <RadioButton
            android:id="@+id/dailibaob"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="10dp"
            android:button="@null"
            android:drawablePadding="5dp"
            android:text="代理报表:"
            android:textSize="13sp" />

        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="7dp"
            android:paddingRight="7dip"
            android:paddingTop="2dip"
            android:paddingBottom="2dip"
            android:text="进入"
            android:textColor="@color/white"
            android:layout_alignBaseline="@+id/dailibaob"
            android:layout_alignBottom="@+id/dailibaob"
            android:layout_centerVertical="true"
            android:layout_marginLeft="5dp"
            android:layout_toEndOf="@+id/dailibaob"
            android:background="@drawable/radius_imageview"
            android:gravity="center_vertical"
            android:textSize="13sp" />

    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:id="@+id/daililist"
        android:visibility="gone"
        android:layout_marginTop="5dp"
        android:background="@color/topbagcolor" />
    <RadioButton
        android:id="@+id/textView2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:button="@null"
        android:textColor="@color/gray8"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/listview"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        android:layout_marginTop="15dp"
        android:drawablePadding="5dip"
        android:drawableLeft="@drawable/warn"
        android:text="@string/tips3"
        android:textSize="13sp" />
</LinearLayout>
